#include<stdio.h>
#include<math.h>
int main(int argc, char *argv[])
{
	double F,a,b,c,x;
	printf("da valori pentru urmatoarele parametri\n");
	printf("a="); scanf("%lf", &a);
	printf("b="); scanf("%lf", &b);
	printf("c="); scanf("%lf",& c);
	printf("x="); scanf("%lf", &x);
	if(x>b+c && a!=0){
		F=sqrt(a+c+b*b)/cos(x);
		printf("F=%lf", F );
	}
		else if(x<b+c && a==0){
			F=(log(a)+log(b))/(a*x+c*c);
			printf("F=%lf", F);
	}		
	else{F=(a+b+x)/(x*x);
	printf("F=%lf", F);
	
	} 
}
